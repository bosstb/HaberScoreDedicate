                    <a href="" class="ng-binding">4</a>
                </li><!-- end ngIf: index.key<=5 --><!-- end ngRepeat: index in tabPage --><!-- ngIf: index.key<=5 --><li ng-click="selectPage(index.key)" ng-if="index.key<=5" ng-repeat="index in tabPage" ng-class="index.key == currpage ? 'active' : 'canclick' " class="ng-scope canclick">
                    <a href="" class="ng-binding">5</a>
                </li><!-- end ngIf: index.key<=5 --><!-- end ngRepeat: index in tabPage -->
                <li ng-click="selectPage(currpage+1)" ng-class="currpage != maxPage ? 'canclick' : '' " class="canclick"><a href="">›</a></li>
                <li ng-click="selectPage(maxPage)" ng-class="currpage != maxPage ? 'canclick' : '' " class="canclick"><a href="">»</a></li>
            </ul><!-- end ngIf: currpage <= 3 -->

            <!-- 小于 5 页情况(尾部) (后来加的，当总页数为4的时候显示不正常，应该是少了这个判断)-->
            <!-- ngIf: (maxPage <= 5) && (currpage >3) -->

            <!-- 大于 5 页情况(中间) -->
            <!-- ngIf: (maxPage > 5) && (currpage > 3) && (currpage < maxPage-2) -->

            <!-- 大于 5 页情况(尾部) -->
            <!-- ngIf: (maxPage > 5) && (currpage >= maxPage-2) -->

        </div>

    </div>
</div></j-pager>
                            </div>
                        </div>
                    </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


